{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Tres Debug mode",
      "skipFiles": [
        "<node_internals>/**",
      ],
      "resolveSourceMapLocations": [
        "!**/node_modules/**"
      ],
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}"
    }
  ]
}