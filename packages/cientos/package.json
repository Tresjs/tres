{
  "name": "@tresjs/cientos",
  "type": "module",
  "version": "5.0.0",
  "packageManager": "pnpm@10.17.0",
  "description": "Collection of useful helpers and fully functional, ready-made abstractions for Tres",
  "author": "Alvaro Saburido <hola@alvarosaburido.dev> (https://github.com/alvarosabu/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tresjs/cientos.git"
  },
  "keywords": [
    "vue",
    "3d",
    "threejs",
    "three",
    "threejs-vue",
    "composables"
  ],
  "maintainers": [
    "Alvaro Saburido (https://github.com/alvarosabu/)",
    "Jaime Torrealba (https://github.com/JaimeTorrealba)",
    "Andre Tchen (https://github.com/andretchen0)"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/trescientos.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts"
    },
    "./*": "./*"
  },
  "main": "./dist/trescientos.js",
  "module": "./dist/trescientos.js",
  "types": "./dist/index.d.ts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "cd playground/vue && pnpm dev",
    "dev:nuxt": "cd playground/nuxt && pnpm dev",
    "build": "vite build",
    "release": "release-it",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit",
    "lint:fix": "eslint . --fix",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "peerDependencies": {
    "@tresjs/core": ">=5.0.0",
    "three": ">=0.133",
    "vue": ">=3.5.17"
  },
  "dependencies": {
    "@vueuse/core": "catalog:",
    "camera-controls": "^2.9.0",
    "stats-gl": "^2.0.1",
    "stats.js": "^0.17.0",
    "three-custom-shader-material": "catalog:",
    "three-stdlib": "catalog:"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^10.0.1",
    "@tresjs/core": "workspace:*",
    "@tresjs/eslint-config": "workspace:*",
    "@types/node": "^24.3.0",
    "@types/three": "catalog:",
    "@typescript-eslint/eslint-plugin": "catalog:",
    "@typescript-eslint/parser": "catalog:",
    "@vitejs/plugin-vue": "catalog:",
    "eslint": "catalog:",
    "eslint-plugin-vue": "catalog:",
    "gsap": "catalog:",
    "kolorist": "catalog:",
    "pathe": "catalog:",
    "release-it": "catalog:",
    "rollup-plugin-analyzer": "catalog:",
    "rollup-plugin-visualizer": "catalog:",
    "three": "catalog:",
    "typescript": "catalog:",
    "unocss": "^66.1.2",
    "vite": "catalog:",
    "vite-plugin-banner": "catalog:",
    "vite-plugin-dts": "catalog:",
    "vite-plugin-glsl": "^1.5.1",
    "vite-svg-loader": "^5.1.0",
    "vitepress": "catalog:",
    "vue-tsc": "catalog:"
  }
}
