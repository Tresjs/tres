    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vitest/ui": "^0.30.1",
    "@vueuse/core": "^10.1.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-vue": "^9.11.0",
    "histoire": "^0.16.1",
